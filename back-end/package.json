{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/src/server.js",
    "build": "rimraf ./build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules --respawn --transpile-only ./src/server.ts",
    "dev:debug": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9230 --notify --poll --respawn --transpile-only ./src/server.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
